from statistics import quantiles
#strings
first_name="Bro"
food="pizza"
email="arunsodhi16@gmail.com"
print(f"Hello {first_name}")
print(f"you like  {food}")
print(f"your email is {email}")
#strings

#intergers
age=24
quantiles= 3
number_of_students=30
print(f"your age is {age}")
print(f"your are buying{quantiles}items")
print(f"you class has {number_of_students} students")
#floats
price=10.99
gpa=3.2
distance=5.5
print(f"the price is$ {price}")
print(f"the gpa is {gpa}")
print(f"you run {distance}km")
#booleans
is_student = False
for_sale = False
is_online = True

if is_online:
    print("You are online")
else:
    print("You are offline")
